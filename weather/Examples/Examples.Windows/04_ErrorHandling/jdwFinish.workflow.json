{
  "TYPE" : "Workflow",
  "version" : "1.0.0",
  "timeZone" : "Etc/UTC",
  "title" : "Prematurely completes a workflow based on dynamic job return values",
  "documentationName" : "PKB-JS7-OnlineDemo-Workflow-jdFinish",
  "orderPreparation" : {
    "parameters" : {
      "return_value_job1" : {
        "type" : "Number",
        "default" : "0"
      }
    },
    "allowUndeclared" : false
  },
  "jobResourceNames" : [ "Default" ],
  "instructions" : [ {
    "TYPE" : "Execute.Named",
    "jobName" : "job1",
    "label" : "job1"
  }, {
    "TYPE" : "If",
    "predicate" : "$return_value_job1 > 0",
    "then" : {
      "instructions" : [ {
        "TYPE" : "Execute.Named",
        "jobName" : "job2",
        "label" : "job2"
      }, {
        "TYPE" : "Finish"
      } ]
    }
  }, {
    "TYPE" : "Execute.Named",
    "jobName" : "job3",
    "label" : "job3"
  } ],
  "jobs" : {
    "job1" : {
      "agentName" : "windowsAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "@echo off\n@echo using workflow: %JS7_WORKFLOW_NAME%\n@echo running job: %JS7_JOB_NAME%\n\nping -n 6 localhost > NUL",
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    },
    "job2" : {
      "agentName" : "windowsAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "@echo off\n@echo using workflow: %JS7_WORKFLOW_NAME%\n@echo running job: %JS7_JOB_NAME%\n\n@echo return value job1: %RETURN_VALUE_JOB1%\n\nping -n 6 localhost > NUL",
        "env" : {
          "RETURN_VALUE_JOB1" : "$return_value_job1"
        },
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    },
    "job3" : {
      "agentName" : "windowsAgent",
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "@echo off\n@echo using workflow: %JS7_WORKFLOW_NAME%\n@echo running job: %JS7_JOB_NAME%\n\n@echo return value job1: %RETURN_VALUE_JOB1%\n\nping -n 6 localhost > NUL\n",
        "env" : {
          "RETURN_VALUE_JOB1" : "$return_value_job1"
        },
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 15,
      "failOnErrWritten" : false
    }
  }
}